{
  "entry": "src/index.js",
  "extraBabelPlugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties",
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  },
  "externals": {
    
  },
  "ignoreMomentLocale": true,
  "theme": "./src/theme.js",
  "hash": true,
  "proxy": {
    "/admin": {
      "target": "http://10.0.10.2:3000/",
      "changeOrigin": true
    }
  }
}
